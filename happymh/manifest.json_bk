{
  "name": "DeleteAD",
  "version": "1.0",
  "description": "Bye! AD",
  "manifest_version": 3,
  "icons": {
    "128": "images/icon.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "cookies",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}